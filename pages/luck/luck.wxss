/* pages/luck/luck.wxss */
.contain{
  padding: 0 30rpx;
  text-align: center;
}
.nav{
  display: block;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    font-size: 32rpx;
    text-align: left;
    justify-content: space-between;
}
.title{
  display: inline-block;
    width: 80%;
    float: left; 
    color:#282828;
    font-weight: 200;
}
.frame_view{
  margin-top: 20rpx;
  padding: 30rpx;
   box-shadow:1px 0px 8px #ededed;
}
.frame_item{
  width: 33.33%;
  height: 160rpx;
}
.desc{
  display: block;
  margin: 20rpx 0;
  padding: 10rpx;
  font-size: 24rpx;
  color: #bbbbbb;
   box-shadow:1px 0px 8px #ededed;
}
.det{
  display: inline-block;
  width: 100%;
  margin: 10rpx 0;
  padding: 10rpx 0;
}
.det span{
  color: #ffe300;
  font-size: 30rpx;
}